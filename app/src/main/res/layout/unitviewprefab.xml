            app:layout_constraintStart_toEndOf="@+id/Strength"
            app:layout_constraintTop_toTopOf="@+id/Strength"
            app:srcCompat="@android:drawable/arrow_down_float"
            tools:visibility="visible" />

        <ImageButton
