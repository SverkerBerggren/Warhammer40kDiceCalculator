                    <profiles>
                        <profile id="e147-7202-837d-9ed7::7d5f-40d6-c52b-a8c3" name="Commissar" publicationId="53e9d88f--pubN88319" page="97" hidden="false" typeId="800f-21d0-4387-c943" typeName="Unit">
                            <characteristics>
                                <characteristic name="M" typeId="0bdf-a96e-9e38-7779">6&quot;</characteristic>
                                <characteristic name="WS" typeId="e7f0-1278-0250-df0c">3+</characteristic>
