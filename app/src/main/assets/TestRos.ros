                <selection id="8537-a69b-0cd3-8667" name="Squiggoth" entryId="8421-0cdc-3dc6-7821::e6c8-1855-9421-3cc4" number="1" type="model">
                    <rules>
                        <rule id="8421-0cdc-3dc6-7821::7701-7dcc-672d-7129::de72-9941-3d27-44e9" name="&apos;Ere We Go!" publicationId="11b5-77b9-pubN65537" page="82" hidden="false">
                            <description>You can re-roll charge rolls made for units with this ability.</description>
                        </rule>
                    </rules>
                    <profiles>
                        <profile id="8421-0cdc-3dc6-7821::dfc5-9481-2de5-b155" name="Squiggoth" publicationId="7573-8d1b-acdf-2de8" page="197" hidden="false" typeId="b3a8-0452-7436-44d1" typeName="Transport">
                            <characteristics>
                                <characteristic name="Capacity" typeId="15aa-1916-a38b-d223">This model has a transport capacity of 10 FLASH GITZ or &lt;CLAN&gt; INFANTRY models. It cannot transport MEGA ARMOUR or JUMP PACK models.</characteristic>
                            </characteristics>
                        </profile>
                        <profile id="8421-0cdc-3dc6-7821::fb25-4ad4-8c13-bf02" name="Squiggoth [1] (10-18+ wounds remaining)" publicationId="7573-8d1b-acdf-2de8" page="197" hidden="false" typeId="800f-21d0-4387-c943" typeName="Unit">
