                                <characteristic name="BS" typeId="8010-b879-355a-c137">2+</characteristic>
                                <characteristic name="Attacks" typeId="2e64-2e5c-f4d1-52d3">3</characteristic>
                            </characteristics>
                        </profile>
                        <profile id="e267-b166-4c3f-fa93::9663-63c7-c008-7b4e" name="Pask Russ 2" hidden="false" typeId="e6d5-85c5-7b01-a3c4" typeName="Stat Damage - M/BS/A">
